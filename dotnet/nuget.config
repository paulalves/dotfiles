<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
    <add key="github_topo" value="https://nuget.pkg.github.com/toposolutions/index.json" />
    <add key="github_paulalves" value="https://nuget.pkg.github.com/paulalves/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <github_topo>
      <add key="Username" value="%GH_TOPO_USER%" />
      <add key="ClearTextPassword" value="%GH_TOPO_PAT%" />
    </github_topo>
    <github_paulalves>
      <add key="Username" value="%GH_PAULALVES_USER%" />
      <add key="ClearTextPassword" value="%GH_PAULALVES_PAT%" />
    </github_paulalves>
  </packageSourceCredentials>
</configuration>
